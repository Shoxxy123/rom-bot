		print("starting alter", zone)
   else 
		error("You need to be in mirror alter to start")
   end
-- chests numbers 104529/123978

sendMacro('SetFindPartyState(false)')
changeProfileOption("MAX_TARGET_DIST", 100)
changeProfileOption("LOOT_IN_COMBAT", "true")
function chkzone()
	if zone ~= 218 then
		error("You are no longer in mirror")
	end
end


</onLoad>

